<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<DialogPane xmlns:fx="http://javafx.com/fxml" fx:id="authoriseDialogId"
            fx:controller="ru.loghorrean.veganShop.controllers.dialog.AuthoriseController">
    <content>
        <GridPane alignment="CENTER" hgap="10" vgap="10">
            <Label text="Username" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="username" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Пароль" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <PasswordField fx:id="pass" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
