<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.loghorrean.veganShop.controllers.client.dishCon.DishCompositionController">
   <children>
      <Label fx:id="dishName" layoutX="14.0" layoutY="14.0" prefHeight="44.0" prefWidth="204.0">
         <font>
            <Font name="Arial" size="21.0" />
         </font>
      </Label>
      <TextArea fx:id="dishDesc" editable="false" layoutX="16.0" layoutY="58.0" prefHeight="114.0" prefWidth="323.0" wrapText="true" />
      <GridPane fx:id="dishComposition" layoutX="16.0" layoutY="250.0" prefHeight="227.0" prefWidth="323.0" vgap="10" hgap="10">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
      </GridPane>
      <Label fx:id="compositionLabel" layoutX="16.0" layoutY="209.0" prefHeight="34.0" prefWidth="323.0" text="Состав блюда ">
         <font>
            <Font name="Arial" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
