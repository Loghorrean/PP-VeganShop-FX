<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainVBox" alignment="center" prefHeight="500.0" prefWidth="900.0" spacing="20" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.loghorrean.veganShop.controllers.MainController">
    <Button fx:id="registerButton" layoutX="256.0" layoutY="144.0" mnemonicParsing="false" onAction="#openRegisterDialog" prefHeight="48.0" prefWidth="144.0" text="Зарегистрироваться" />
    <Button fx:id="authoriseButton" layoutX="268.0" layoutY="175.0" mnemonicParsing="false" onAction="#openAuthoriseDialog" prefHeight="48.0" prefWidth="144.0" text="Авторизоваться" />
    <Button fx:id="aboutUsButton" mnemonicParsing="false" onAction="#openAboutDialog" prefHeight="48.0" prefWidth="144.0" text="О нашей компании" />
    <Button fx:id="shutdownButton" layoutX="253.0" layoutY="208.0" mnemonicParsing="false" onAction="#shutdownApplication" prefHeight="48.0" prefWidth="144.0" text="Выйти из программы" />
</VBox>
